@using kursovay.DTO
@{ 
    SelectList selectList = (SelectList)ViewData["list"];
    int orderId = (int)ViewData["orderId"];
    PartnerDto partner = (PartnerDto)Session["session"];
}

@using (Html.BeginForm("MoveOrder", "Order", new { orderId = orderId, userId = partner.Id, role = partner.Role }))
{
    @Html.DropDownList("newWarehouseId", selectList)
    <input type="submit" value="Move order"/>
}
